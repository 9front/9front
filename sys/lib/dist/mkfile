scr=/tmp/cdstub
iso=/tmp/9front.iso

cd:V: $iso
	

clean:V:
	rm -fr $scr $iso

%.iso:	/n/src9/386/9bootiso
	mkdir -p $scr
	cp $prereq $scr
	cp pc/plan9.ini.cd $scr/plan9.ini
	bind -a /n/src9 $scr
	disk/mk9660 -9cj -B 9bootiso -p pc/cdproto -s $scr -v 'Plan 9 Front' $target
	unmount $scr
	rm -fr $scr
