</$objtype/mkfile

TARG=tl

OFILES=\
	asm.$O\
	list.$O\
	noop.$O\
	obj.$O\
	optab.$O\
	pass.$O\
	span.$O\
	enam.$O\
	thumb.$O\
	compat.$O\

